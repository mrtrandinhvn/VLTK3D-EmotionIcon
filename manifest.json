{
    "name": "VLTK 3D Emotions",
    "version": "2.0.0",
    "description": "VLTK3 Emotion importer",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*",
                "http://www.facebook.com/*"
            ],
            "css": [ "css/EmoStyle.css" ],
            "js": [
                "js/jquery-1.11.3.min.js",
                "js/chat-windows.js",
                "js/status.js",
                "js/controller.js",
                "js/main.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": {
            "19": "icons/icon19.png",
            "38": "icons/icon38.png"
        },
        "default_popup": "default_popup.html"
    },
    "manifest_version": 2,
    "author": "Nemu"
}